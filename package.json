{
  "name": "@syfxlin/reve",
  "type": "module",
  "version": "0.0.0-semantic-release",
  "description": "Support for css-prop-like API and flexible theme system for vanilla-extract.",
  "author": "Otstar Lin <i@ixk.me>",
  "license": "MIT",
  "homepage": "https://github.com/syfxlin/reve#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/syfxlin/reve.git"
  },
  "bugs": {
    "url": "https://github.com/syfxlin/reve/issues"
  },
  "keywords": [
    "vanilla-extract",
    "styled-components",
    "css-in-js",
    "css"
  ],
  "publishConfig": {
    "access": "public"
  },
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.mts",
        "default": "./dist/index.mjs"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    }
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "vite",
    "build": "bunchee",
    "watch": "bunchee --watch"
  },
  "peerDependencies": {
    "@vanilla-extract/css": "^1.12.0"
  },
  "dependencies": {
    "@vanilla-extract/css-utils": "^0.1.3",
    "@vanilla-extract/dynamic": "^2.1.0",
    "clsx": "^2.1.0",
    "stylis": "^4.3.1"
  },
  "devDependencies": {
    "@syfxlin/eslint-config": "^1.0.4",
    "@types/react": "^18.2.63",
    "@types/react-dom": "^18.2.20",
    "@types/stylis": "^4.2.5",
    "@vanilla-extract/vite-plugin": "^4.0.4",
    "@vitejs/plugin-react": "^4.2.1",
    "bunchee": "^5.0.0",
    "eslint": "^8.57.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "typescript": "^5.3.3",
    "vite": "^5.1.5"
  }
}
